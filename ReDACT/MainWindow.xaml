        xmlns:local="clr-namespace:ReDACT"
        mc:Ignorable="d"
        Title="ReDACT" Height="350" Width="525">
    <Grid>
        <Button Content="Button" HorizontalAlignment="Left" Margin="267,65,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
